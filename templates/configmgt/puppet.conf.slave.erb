[main]
vardir = /var/lib/puppet
logdir = /var/log/puppet
rundir = /var/run/puppet
ssldir = $vardir/ssl
[agent]
pluginsync      = true
report          = false
ignoreschedules = true
daemon          = false
ca_server       = <%=@masteralias%>
certname        = <%=@alces_hostname%>
environment     = production
server          = <%=@masteralias%>
